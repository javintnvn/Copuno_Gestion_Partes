# Configuración de Notion API
# Sustituye por tu token de integración de Notion (no compartas este valor)
NOTION_TOKEN=YOUR_NOTION_TOKEN

# Modo mock para pruebas locales sin conectar con Notion
# USE_MOCK_DATA=true

# Configuración del servidor
PORT=3001
NODE_ENV=development

# Webhook para exportar partes (opcional)
# PARTES_DATOS_WEBHOOK_URL=https://tuwebhook.com/evento
# PARTES_WEBHOOK_TIMEOUT_MS=10000

# (opcional) CORS en producción: lista separada por comas de orígenes permitidos
# ALLOWED_ORIGINS=https://tudominio.com,https://app.tudominio.com 

# (opcional) Rate limiting
# RATE_LIMIT_WINDOW_MS=900000
# RATE_LIMIT_MAX=100
